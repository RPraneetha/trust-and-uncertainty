{
  "name": "trust-and-uncertainty",
  "version": "3.0.0",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "install:all": "cd api/ && npm install && cd ../client && npm install",
    "start": "cd api && npm install && node bin/www",
    "build": "cd client/ && npm install && npm run build",
    "start:client": "cd client/ && npm start",
    "start:dev": " npm start && npm run start:client"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules",
    "api/node_modules"
  ],
  "dependencies": {
    "express": "^4.16.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RPraneetha/trust-and-uncertainty"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
